@{
    ViewData["Title"] = "Manufacturer Create";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

@using BOTY.Models.Tables
@model Manufacturer

<h1>Manufacturer Shoe</h1>

<form asp-action="ManufacturerCreate">
